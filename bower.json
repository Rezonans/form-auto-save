  "dependencies": {
    "jquery": "~2.1.1",
    "lodash": "~2.4.1",
    "bootstrap": "~3.3.1",
    "angular": "~1.3.2",
    "angular-bootstrap": "~0.11.2",
    "angular-route": "latest",
    "angular-ui-utils": "~0.1.1",
    "ngstorage": "~0.3.0",
    "angular-cookies": "~1.3.2",
    "angular-sanitize": "~1.3.2",
    "angular-local-storage": "~0.1.5"
  },
  "overrides": {
